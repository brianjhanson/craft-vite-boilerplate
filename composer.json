{
  "repositories": [],

  "require": {
    "craftcms/cms": "^3.7.0",
    "craftcms/redactor": "^2.10",
    "marionnewlevant/twig-perversion": "^2.2",
    "nystudio107/craft-autocomplete": "^1.10",
    "nystudio107/craft-emptycoalesce": "^1.0",
    "topshelfcraft/wordsmith": "3.3.0.1",
    "vlucas/phpdotenv": "^5.4.0"
